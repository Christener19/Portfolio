{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/contact",
        "destination": "/api/contact.js",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Content-Type", "value": "application/json" }
        ]
      },
      {
        "source": "/:path*",
        "has": [
          {
            "type": "query",
            "key": "authorized"
          }
        ],
        "destination": "/api/index.js"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": true,
    "installCommand": "npm install"
  }
  
  